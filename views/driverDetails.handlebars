            <th>liscense number</th>
            <th>email</th>
            <th>address</th>
            <th>dob</th>
            <th>gender</th>
            <th>password</th>


        </tr>
        
        {{#each driverDetails}}
        <tr>
            <td>{{this.dataValues.Driver_id}}</td>
            <td>{{this.dataValues.Drive_name}}</td>
