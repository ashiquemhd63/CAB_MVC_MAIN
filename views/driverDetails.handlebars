            <td>{{this.dataValues.Driver_email}}</td>
            <td>{{this.dataValues.Driver_adress}}</td>
            <td>{{this.dataValues.Driver_dob}}</td>
            <td>{{this.dataValues.Driver_gender}}</td>
            <td>{{this.dataValues.Driver_password}}</td>

            <td>
                <a href="editdriver/{{this.dataValues.Driver_id}}">Edit</a>ðŸš—
                <a href="deletedriver/{{this.dataValues.Driver_id}}" onclick="return confirm('Are you sure to delete this? ')">Delete</a>
